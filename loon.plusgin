#!name=访问私库
#!desc=允许用户访问自定义的 GitHub/Gist 私有仓库
[Rewrite]
^https?:\/\/(raw|gist)\.githubusercontent\.com\/GITHUB_USERNAME header-del Authorization
^https?:\/\/(raw|gist)\.githubusercontent\.com\/GITHUB_USERNAME header-add Authorization "token GITHUB_TOKEN"
[MITM]
hostname = raw.githubusercontent.com, gist.githubusercontent.com