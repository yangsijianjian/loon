#!name=访问私库
#!desc=允许用户访问自定义的 GitHub/Gist 私有仓库
#!author=soya
#!version=1.0

[Rewrite]
http-request ^https?:\/\/(raw|gist)\.githubusercontent\.com\/你的用户名 header-del Authorization
http-request ^https?:\/\/(raw|gist)\.githubusercontent\.com\/你的用户名 header-add Authorization = token 你的Token

[MITM]
hostname = raw.githubusercontent.com, gist.githubusercontent.com